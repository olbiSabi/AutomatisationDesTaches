+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo select 'SPE_MNGR_MNGE;'||TRIM(T3.MATCLE)||';'||TRIM(T1.MATCLE) 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo ||';;CDC_MNG;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
