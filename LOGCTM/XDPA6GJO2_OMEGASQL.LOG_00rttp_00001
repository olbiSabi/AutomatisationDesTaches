+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo WHERE T1.NUDOSS  = T7.NUDOSS  AND T1.NUDOSS = T8.NUDOSS 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND T1.NUDOSS = T3.NUDOSS AND T1.nudoss = T4.nudoss 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T8.DATCON,'YYYY-MM-DD') <= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T8.DATFIN,'YYYY-MM-DD') >= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T4.dtef00,'YYYY-MM-DD') <= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T4.dten00,'YYYY-MM-DD') >= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND   T1.TYPDOS = 'SAL' AND (T7.STATUT = '3' OR T7.STATUT = '4')  
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND T8.TYPCON <> 'Z' AND T8.NATCON NOT IN ('CT','CV','CE','CA','AO')  
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
