# =========================================================================== #
# 18-03-2021 a 23:31:03 fl_lance_extract : Constitution de la requête d'extraction OMEGASQL
# =========================================================================== #
+echo SET HEAD OFF
+1> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo SET ECHO OFF
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo SET FEEDBACK OFF
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo SET PAGESIZE 0
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo SET LINESIZE 88
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo select 'SPE_MNGR_MNGE;'||TRIM(T3.MATCLE)||';'||TRIM(T1.MATCLE) 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo ||';;CDC_MNG;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo FROM HR.ZY00 T1,HR.ZYVK T7,HR.ZYCO T8,HR.ZY3E T3,HR.ZY3B T4,HR.Ze00 T5
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo WHERE T1.NUDOSS  = T7.NUDOSS  AND T1.NUDOSS = T8.NUDOSS 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND T1.NUDOSS = T3.NUDOSS AND T1.nudoss = T4.nudoss 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T8.DATCON,'YYYY-MM-DD') <= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T8.DATFIN,'YYYY-MM-DD') >= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T4.dtef00,'YYYY-MM-DD') <= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND TO_CHAR(T4.dten00,'YYYY-MM-DD') >= '2021-03-18' 
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND   T1.TYPDOS = 'SAL' AND (T7.STATUT = '3' OR T7.STATUT = '4')  
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND T8.TYPCON <> 'Z' AND T8.NATCON NOT IN ('CT','CV','CE','CA','AO')  
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND T4.IDOU00 = T5.IDOU00                              
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND not exists (select 1 from ZE2A T9 where T9.IDOU00 = T5.IDOU00     
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo AND T9.IDOU01 = '001468');                             
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
+echo EXIT;
+1>> /dz-pareo/applis/pa6/sql/OMEGASQL.sql
